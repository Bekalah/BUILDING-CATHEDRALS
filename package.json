{
  "name": "cathedral-monorepo",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx,js,jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "standards": "npm run lint && npm run type-check && npm run format:check",
    "standards:fix": "npm run lint:fix && npm run format",
    "codex:validate": "node scripts/validate-codex.mjs",
  "codex:enrich:music": "node scripts/enrich-codex-music.mjs",
  "codex:enrich:tarot": "node scripts/enrich-codex-tarot-audio.mjs",
    "react-latex": "^2.0.0"
  "react-image-fallback": "^1.0.0"
}
}
